{
    "name": "gamemakerhub/gmmodular",
    "description": "An extension manager for Game Maker Studio 2",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^4.3",
        "symfony/flex": "^1.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "RobQuistNL",
            "email": "rob.quist@dukesoft.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "GMM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GMM\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3@dev"
    },
    "bin": ["bin/phpunit"]
}
