{
    "name": "gamemakerhub/gmdepman",
    "description": "An extension manager for Game Maker Studio 2",
    "type": "project",
    "require": {
        "php": "^7.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^4.3",
        "phpunit/phpunit": "^8.0",
        "symfony/dependency-injection": "^4.3",
        "symfony/config": "^4.3",
        "symfony/yaml": "^4.3",
        "symfony/http-kernel": "^4.3",
        "ramsey/uuid": "^4.0@dev",
        "composer/semver": "^1.0@dev"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "RobQuistNL",
            "email": "rob.quist@dukesoft.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "GMDepMan\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GMDepMan\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require-dev": {
        "beberlei/assert": "dev-master",
        "symfony/phpunit-bridge": "^4.3",
        "mockery/mockery": "^1.0@dev"
    }
}
