{
    "name": "gamemakerhub/catalyst",
    "description": "A tool for Game Maker Studio 2 to manage dependencies and speed up your process",
    "type": "project",
    "config": {
        "platform": {
            "php": "7.2"
        }
    },
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^4.2",
        "beberlei/assert": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "composer/semver": "^1.5",
        "ramsey/uuid": "^3.8",
        "symfony/dependency-injection": "^4.2",
        "symfony/config": "^4.2",
        "symfony/yaml": "^4.2",
        "symfony/process": "^4.2",
        "symfony/http-kernel": "^4.2",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "RobQuistNL",
            "email": "rob.quist@dukesoft.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Catalyst\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Catalyst\\Tests\\": "tests/src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.0",
        "symfony/phpunit-bridge": "^4.2"
    }
}
